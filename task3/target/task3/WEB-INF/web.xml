<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>myservlet</servlet-name>
    <servlet-class>com.clouway.Dispatcher</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>myservlet</servlet-name>
    <url-pattern>/myservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Abv</servlet-name>
    <servlet-class>com.clouway.Abv</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Abv</servlet-name>
    <url-pattern>/abv.html</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Yahoo</servlet-name>
    <servlet-class>com.clouway.Yahoo</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Yahoo</servlet-name>
    <url-pattern>/yahoo.html</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Gmail</servlet-name>
    <servlet-class>com.clouway.Gmail</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Gmail</servlet-name>
    <url-pattern>/gmail.html</url-pattern>
  </servlet-mapping>
</web-app>
